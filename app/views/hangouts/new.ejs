<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.hangouts() %>">hangouts</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New hangout</a></li>
</ul>

<div class="page-header">
    <h1>New hangout</h1>
</div>

<% var form = formFor(hangout, {action: pathTo.hangouts(), method: 'POST', id: "hangout-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create hangout', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.hangouts(), {class: 'btn'}) %>
</div>
<%- form.end() %>
